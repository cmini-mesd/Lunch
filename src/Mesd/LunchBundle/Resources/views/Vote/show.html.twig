{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Vote</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Voter_id</th>
                <td>{{ entity.voterid }}</td>
            </tr>
            <tr>
                <th>Vote_date</th>
                <td>{{ entity.votedate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('vote') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('vote_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
